/**app.wxss**/
/*全局样式，会作用到所有wxml  */
.container {
  height: 100%;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: space-between;
  padding: 200rpx 0;
  box-sizing: border-box;
} 
.h1{
  font-weight: bold;
  font-size: 20px;
  text-align: center;
}

.btu{
  border: 2px solid black;
  transition: all 0.3s;
}

input{
  border: 1px solid black;
}

@import "weui.wxss";